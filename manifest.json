{
  "manifest_version": 2,

  "name": "Cryptsy orders notifier",
  "description": "Notify abount changes in orders on Cryptsy",
  "version": "1.0",
	
 "background": {
	"page": "background.html"
  },

  "permissions": [
	"storage",
    "notifications",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  }
}
